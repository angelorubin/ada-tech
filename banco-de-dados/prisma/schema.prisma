generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Disciplina {
  id            Int      @id @default(autoincrement())
  id_disciplina Int
  nome          String
  ementa        String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  @@map("disciplina") // Mapear para tabela em min√∫sculas
}
